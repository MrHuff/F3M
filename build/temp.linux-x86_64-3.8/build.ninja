rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags



build /home/rhu/Documents/phd_projects/cuda_n_body/build/temp.linux-x86_64-3.8/n_tree.o: cuda_compile /home/rhu/Documents/phd_projects/cuda_n_body/n_tree.cu





